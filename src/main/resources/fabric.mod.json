	"entrypoints": {
		"main": [
			"io.github.myworldzycpc.quick_phrases.QuickPhrases"
		],
		"client": [
			"io.github.myworldzycpc.quick_phrases.QuickPhrasesClient"
		]
	},
	"mixins": [
		"quick-phrases.mixins.json",
		{
			"config": "quick-phrases.client.mixins.json",
