{
	"schemaVersion": 1,
	"id": "quick-phrases",
	"version": "${version}",
	"name": "Quick Phrases",
	"description": "A mod that adds quick phrases to chat",
	"authors": [
		"myworldzycpc"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/quick-phrases/icon.png",
